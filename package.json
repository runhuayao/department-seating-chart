{
  "name": "department-map",
  "private": true,
  "version": "3.1.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "server-management:dev": "vite --config vite.server-management.config.ts --mode development",
    "build": "tsc -b && vite build",
    "build:server-management": "tsc -b && vite build --config vite.server-management.config.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:server-management": "vite preview --config vite.server-management.config.ts",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "redis:start": "scripts\\start-redis-simple.cmd",
    "redis:start-force": "pwsh -ExecutionPolicy Bypass -File scripts/start-redis.ps1 -Force",
    "services:check": "scripts\\start-redis-simple.cmd",
    "services:start": "scripts\\start-redis-simple.cmd",
    "predev": "npm run services:start",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "dev:all": "concurrently \"npm run client:dev\" \"npm run server-management:dev\" \"npm run server:dev\"",
    "db:migrate": "node scripts/migrate.js",
    "db:rollback": "node scripts/migrate.js --rollback",
    "db:status": "node scripts/migrate.js --status",
    "db:migrate-m0": "node scripts/migrateM0Data.js",
    "db:migrate-m0-force": "node scripts/migrateM0Data.js --force",
    "db:validate": "node scripts/validateData.js",
    "db:setup": "node scripts/setupDatabase.js",
    "db:setup-clean": "node scripts/setupDatabase.js --force",
    "db:setup-schema-only": "node scripts/setupDatabase.js --skip-m0",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "postbuild": "echo 'Build completed successfully'",
    "start": "node api/server.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.9.3",
    "redis": "^4.6.12",
    "socket.io": "^4.7.4",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/d3-zoom": "^3.0.8",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.15.30",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
